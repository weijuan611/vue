B,GAAA,mBAAAyB,EAAAzB,GAAA,KAAAJ,EAAA,OAAkDE,YAAA,aAAuBF,EAAA,WAAgB4F,IAAA,OAAAvF,OAAkBwF,MAAAhE,EAAAiE,KAAAC,cAAA,UAAuC/F,EAAA,gBAAqBK,OAAO2F,MAAA,UAAgBhG,EAAA,YAAiB6F,OAAOI,MAAApE,EAAAiE,KAAA,KAAAI,SAAA,SAAAC,GAA+CtE,EAAAuE,KAAAvE,EAAAiE,KAAA,OAAAK,IAAgCE,WAAA,gBAAyB,GAAAxE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAqCK,OAAO2F,MAAA,SAAehG,EAAA,aAAkBK,OAAOiG,YAAA,OAAoBT,OAAQI,MAAApE,EAAAiE,KAAA,OAAAI,SAAA,SAAAC,GAAiDtE,EAAAuE,KAAAvE,EAAAiE,KAAA,SAAAK,IAAkCE,WAAA,iBAA2BrG,EAAA,aAAkBuC,IAAA,MAAAlC,OAAiB2F,MAAA,MAAAC,MAAA,SAA6BpE,EAAAzB,GAAA,KAAAJ,EAAA,aAA8BuC,IAAA,MAAAlC,OAAiB2F,MAAA,MAAAC,MAAA,SAA6BpE,EAAAzB,GAAA,KAAAJ,EAAA,aAA8BuC,IAAA,OAAAlC,OAAkB2F,MAAA,OAAAC,MAAA,WAA+B,OAAApE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAyCK,OAAO2F,MAAA,UAAgBhG,EAAA,UAAeK,OAAOkG,KAAA,MAAWvG,EAAA,kBAAuBwG,aAAaC,MAAA,QAAepG,OAAQqG,KAAA,OAAAJ,YAAA,QAAmCT,OAAQI,MAAApE,EAAAiE,KAAA,MAAAI,SAAA,SAAAC,GAAgDtE,EAAAuE,KAAAvE,EAAAiE,KAAA,QAAAK,IAAiCE,WAAA,iBAA0B,GAAAxE,EAAAzB,GAAA,KAAAJ,EAAA,UAA+BE,YAAA,OAAAG,OAA0BkG,KAAA,KAAU1E,EAAAzB,GAAA,OAAAyB,EAAAzB,GAAA,KAAAJ,EAAA,UAAyCK,OAAOkG,KAAA,MAAWvG,EAAA,kBAAuBwG,aAAaC,MAAA,QAAepG,OAAQqG,KAAA,aAAAJ,YAAA,QAAyCT,OAAQI,MAAApE,EAAAiE,KAAA,MAAAI,SAAA,SAAAC,GAAgDtE,EAAAuE,KAAAvE,EAAAiE,KAAA,QAAAK,IAAiCE,WAAA,iBAA0B,OAAAxE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAyCK,OAAO2F,MAAA,UAAgBhG,EAAA,aAAkBK,OAAOsG,UAAA,GAAAC,WAAA,IAA2Bf,OAAQI,MAAApE,EAAAiE,KAAA,SAAAI,SAAA,SAAAC,GAAmDtE,EAAAuE,KAAAvE,EAAAiE,KAAA,WAAAK,IAAoCE,WAAA,oBAA6B,GAAAxE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAqCK,OAAO2F,MAAA,SAAehG,EAAA,qBAA0B6F,OAAOI,MAAApE,EAAAiE,KAAA,KAAAI,SAAA,SAAAC,GAA+CtE,EAAAuE,KAAAvE,EAAAiE,KAAA,OAAAK,IAAgCE,WAAA,eAAyBrG,EAAA,eAAoBK,OAAO2F,MAAA,MAAAxE,KAAA,UAA6BK,EAAAzB,GAAA,KAAAJ,EAAA,eAAgCK,OAAO2F,MAAA,MAAAxE,KAAA,UAA6BK,EAAAzB,GAAA,KAAAJ,EAAA,eAAgCK,OAAO2F,MAAA,OAAAxE,KAAA,WAA8B,OAAAK,EAAAzB,GAAA,KAAAJ,EAAA,gBAAyCK,OAAO2F,MAAA,SAAehG,EAAA,kBAAuB6F,OAAOI,MAAApE,EAAAiE,KAAA,SAAAI,SAAA,SAAAC,GAAmDtE,EAAAuE,KAAAvE,EAAAiE,KAAA,WAAAK,IAAoCE,WAAA,mBAA6BrG,EAAA,YAAiBK,OAAO2F,MAAA,SAAenE,EAAAzB,GAAA,KAAAJ,EAAA,YAA6BK,OAAO2F,MAAA,SAAenE,EAAAzB,GAAA,KAAAJ,EAAA,YAA6BK,OAAO2F,MAAA,WAAgB,OAAAnE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAyCK,OAAO2F,MAAA,SAAehG,EAAA,YAAiBK,OAAOqG,KAAA,YAAkBb,OAAQI,MAAApE,EAAAiE,KAAA,KAAAI,SAAA,SAAAC,GAA+CtE,EAAAuE,KAAAvE,EAAAiE,KAAA,OAAAK,IAAgCE,WAAA,gBAAyB,GAAAxE,EAAAzB,GAAA,KAAAJ,EAAA,gBAAAA,EAAA,aAAqDK,OAAOqG,KAAA,WAAiBnG,IAAKiC,MAAAX,EAAAgF,YAAsBhF,EAAAzB,GAAA,QAAAyB,EAAAzB,GAAA,KAAAJ,EAAA,aAAA6B,EAAAzB,GAAA,uBAEt3FW,oBCWjBwE,EAbA5E,EAAA,OAaAM,oCFkDA,UACA,SACA,SACA,aACA,QACA,gBACA,WAGA,yDAIA,YE7DA+J,GATA,EAEA,KAEA,KAEA,MAUA5F,EAAA,QAAAG,EAAA","file":"static/js/app.1d48334e322602dd4edc.js","sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <div class=\"logo\"><img src=\"./../assets/img/logo.png\" />后台管理系统</div>\r\n    <div class=\"user-info\">\r\n      <el-dropdown trigger=\"click\" @command=\"handleCommand\">\r\n        <span class=\"el-dropdown-link\">\r\n          <img class=\"user-logo\" src=\"./../assets/img/head2.jpg\">\r\n          {{username}}\r\n        </span>\r\n        <el-dropdown-menu slot=\"dropdown\">\r\n          <el-dropdown-item command=\"loginout\">退出</el-dropdown-item>\r\n        </el-dropdown-menu>\r\n      </el-dropdown>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data () {\r\n      return {\r\n        initUsername: '游客登录'\r\n      }\r\n    },\r\n    computed: {\r\n      username () {\r\n        let myUsername = localStorage.getItem('ms_username')   // 对象  没有是为null\r\n        return myUsername !== null ? myUsername : this.initUsername\r\n      }\r\n    },\r\n    methods: {\r\n      handleCommand (command) {\r\n        if (command === 'loginout') {\r\n          localStorage.removeItem('ms_username')\r\n          this.$router.push('/login')\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\n\n\n// WEBPACK FOOTER //\n// src/components/Header.vue","<template>\r\n  <div class=\"topbar clearfix\">\r\n    <a v-if=\"seen\" class=\"toPrev\" @click=\"toPrevFunc($event)\"><i class=\"el-icon-d-arrow-left\"></i></a>\r\n    <div class=\"menu\">\r\n      <el-menu\r\n        :style=\"{left: '0px'}\"\r\n        :default-active=\"activeIndex\"\r\n        class=\"el-menu-demo\"\r\n        mode=\"horizontal\"\r\n        background-color=\"#545c64\"\r\n        text-color=\"#fff\"\r\n        active-text-color=\"#ffd04b\"\r\n        unique-opened\r\n        router>\r\n        <template v-for=\"item in items\">\r\n          <el-menu-item\r\n            :index=\"item.index\"\r\n            :id=\"item.index\"\r\n            :key=\"item.index\"\r\n            @click=\"clickFunc($event)\"\r\n            @mouseover.native=\"mouseoverFunc($event)\"\r\n            @mouseout.native=\"mouseoutFunc($event)\">\r\n            {{item.title}}\r\n          </el-menu-item>\r\n        </template>\r\n      </el-menu>\r\n    </div>\r\n    <a v-if=\"seen\" class=\"toNext\" @click=\"toNextFunc($event)\"><i class=\"el-icon-d-arrow-right\"></i></a>\r\n    <a class=\"urlList\">\r\n      <el-dropdown trigger=\"click\" @command=\"handleCommand\">\r\n        <span class=\"el-dropdown-link\"><i class=\"el-icon-more\"></i></span>\r\n        <el-dropdown-menu slot=\"dropdown\">\r\n          <template v-for=\"item in items\">\r\n            <el-dropdown-item :command=\"item.index\">\r\n              {{item.title}}\r\n            </el-dropdown-item>\r\n          </template>\r\n        </el-dropdown-menu>\r\n      </el-dropdown>\r\n    </a>\r\n    <ul id=\"rightMenu\">\r\n      <!-- <li><i class=\"el-icon-refresh\"> 刷新本标签</i></li> -->\r\n      <li @click=\"closeSelfTabFunc\"><i class=\"el-icon-close\"> 关闭本标签</i></li>\r\n      <li @click=\"closeOtherTabFunc\"><i class=\"el-icon-close\"> 关闭其它标签</i></li>\r\n      <li @click=\"closeAllTabFunc\"><i class=\"el-icon-close\"> 关闭所有标签</i></li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import $fn from './../assets/js/baseFunc/index'\r\n  var $ul\r\n  var viewUl\r\n  var rightMenu\r\n\r\n  export default {\r\n    data () {\r\n      return {\r\n        items: [\r\n          {\r\n            index: 'index',\r\n            title: '工作台'\r\n          }\r\n        ],\r\n        activeIndex: 'index',\r\n        activeIndexTemp: 'index',\r\n        closedIndex: '',\r\n        lisWidth: 0,\r\n        seen: false\r\n      }\r\n    },\r\n    methods: {\r\n      closeSelfTabFunc: function () {   // 关闭本标签页\r\n        if (this.closedIndex === 'index') {\r\n          return\r\n        }\r\n        this.$router.push({\r\n          path: this.closedIndex\r\n        })\r\n        for (let i = 0; i < this.items.length; i++) {\r\n          if (this.items[i].index === this.closedIndex) {\r\n            this.items.splice(i, 1)\r\n            this.activeIndexTemp = this.items[i - 1].index\r\n          }\r\n        }\r\n        this.$root.Bus.$emit('url-from-topbar', this.activeIndexTemp)  // 向兄弟组件传值\r\n      },\r\n      closeOtherTabFunc: function () {   // 关闭其它标签页\r\n        this.$router.push({\r\n          path: this.closedIndex\r\n        })\r\n        this.activeIndexTemp = this.closedIndex\r\n        this.$root.Bus.$emit('url-from-topbar', this.closedIndex)  // 向兄弟组件传值\r\n\r\n        if (this.closedIndex === 'index') {\r\n          this.items.splice(1, this.items.length - 1)\r\n          return\r\n        }\r\n\r\n        let temp\r\n        for (let i = 0; i < this.items.length; i++) {\r\n          if (this.closedIndex === this.items[i].index) {\r\n            temp = this.items[i]\r\n            this.items[i] = this.items[1]\r\n            this.items[1] = temp\r\n          }\r\n        }\r\n        this.items.splice(2, this.items.length - 2)\r\n      },\r\n      closeAllTabFunc: function () {  //  关闭所有标签页\r\n        this.items.splice(1, this.items.length - 1)\r\n        this.$router.push({\r\n          path: 'index'\r\n        })\r\n        this.activeIndexTemp = 'index'\r\n        this.$root.Bus.$emit('url-from-topbar', this.activeIndexTemp)  // 向兄弟组件传值\r\n      },\r\n      toPrevFunc: function (e) {    // 标签页向左移\r\n        let 